
---
import Base from "../../../layouts/Base.astro";
const title = "Advertise";
const subtitle = "Reach locals with GoDoDaytona.";
---
<Base title={title} subtitle={subtitle} cityBase="daytona" />

<section class="section">
  <div class="container">
    <h1 class="h1">Advertise</h1>
    <p class="lede">Coming soon. Sponsorships, placements, and simple local advertising packages.</p>

    <div class="card card-pad" style="margin-top:14px;">
      <div class="kicker">Contact</div>
      <p class="lede" style="margin:8px 0 0;">
        Email: <a href="mailto:ads@godocity.com">ads@godocity.com</a>
      </p>
    </div>
  </div>
</section>
